var tsxrking = false;
var $this = $('#login');
var $state = $this.find('.login_errors');

<?php if($tsx['config']['googleLogin'] != 0): ?>
function handleCredentialResponse(response) {
   var id_token = response.credential;
   $.post(tsx_Ajax_Requests_File() + '?f=google_login', {id_token: id_token}, function(data, textStatus, xhr) {
      if (data.status == 200) {
         $state.addClass('success');
           $state.html('<?php echo $tsx['lang']['welcome_back'] ?>');
         $this.find('.loading').addClass('hidden');
           setTimeout(function () {
            window.location.href = data.location;
           }, 1000);
      }
      else{
         $this.find('button').attr("disabled", false);
         $this.find('.loading').addClass('hidden');
           $state.html(data.message);
      }
   });
}
window.onload = function () {
  google.accounts.id.initialize({
    client_id: "<?php echo $tsx['config']['googleAppId']?>",
    callback: handleCredentialResponse
  });
  google.accounts.id.renderButton(
    document.getElementById("buttonDiv"),
    { theme: "outline", size: "large" }  // customization attributes
  );
  google.accounts.id.prompt(); // also display the One Tap dialog
}
<?php endif; ?>

$(function() {
  $('#login').ajaxForm({
    url: tsx_Ajax_Requests_File() + '?f=login',
    beforeSend: function() {
      tsxrking = true;
      $this.find('button').attr("disabled", true);
      $this.find('.add_joinda_loader').addClass('btn-loading');
    },
    success: function(data) {
      if (data.status == 200 || data.status == 600) {
            $state.removeClass('alert-danger');
         $state.addClass('alert-success');

        $state.html('<?php echo $tsx['lang']['welcome_back'] ?>');
      $this.find('.add_joinda_loader').removeClass('btn-loading');
        setTimeout(function () {
         window.location.href = data.location;
        }, 1000);
      } else {
        var errors = data.errors.join("<br>");
      $this.find('button').attr("disabled", false);
      $this.find('.add_joinda_loader').removeClass('btn-loading');
        $state.html(errors);
      }
      tsxrking = false;
    }
  });
});



var register_tsxrking = false;
